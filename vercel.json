{
    "version": 2,
    "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/[^.]+$",
        "dest": "/index.html"
      },
      {
        "src": "^/(.*)\\.(.+)$",
        "dest": "/$1.$2"
      }
    ],
    "buildCommand": "CI=false npm run build",
    "outputDirectory": "build"
  }