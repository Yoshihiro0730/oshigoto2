{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "routes": [
      {
        "src": "/manifest.json",
        "dest": "/manifest.json",
        "headers": { "Content-Type": "application/json" }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "build"
  }